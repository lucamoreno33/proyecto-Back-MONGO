<main>
    <h1>Productos:</h1>
    {{#each products}}
        <div>
            <p>producto: {{this.title}}</p>
            <p>{{this.description}}</p>
            <p>precio: {{this.price}}</p>
            <p>codigo de producto: {{this.code}}</p>
            <p>stock {{this.stock}}</p>
        </div>
        <br>
    {{/each}}
</main>
